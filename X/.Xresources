Xft.dpi: XDPI

*background: #121212
*foreground: #9F9FA1

! Black
*color0:     #24292E
*color8:     #24292E
! Red
*color1:     #F02233
*color9:     #F02233
! Green
*color2:     #00FF00
*color10:    #00FF00
! Yellow
*color3:     #FFD75F
*color11:    #FFD75F
! Blue
*color4:     #6479FF
*color12:    #6479FF
! Purple
*color5:     #AF5FFF
*color13:    #AF5FFF
! Cyan
*color6:     #53D3FB
*color14:    #53D3FB
! White
*color7:     #FFFFFF
*color15:    #FFFFFF

! URxvt
URxvt.saveLines:   4096
URxvt.scrollBar:   false
URxvt.letterSpace: -1
URxvt.font:        xft:Hack-Regular:pixelsize=12
URxvt.boldFont:    xft:Hack-Bold:pixelsize=12:weight=bold
URxvt.italicFont:  xft:Hack-RegularOblique:pixelsize=12:slant=italic
