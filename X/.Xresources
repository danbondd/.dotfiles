Xft.dpi: XDPI

*background: #121212
*foreground: #9F9FA1

! Black
*color0:     #24292E
*color8:     #24292E
! Red
*color1:     #F02233
*color9:     #F02233
! Green
*color2:     #00FF00
*color10:    #00FF00
! Yellow
*color3:     #FFD75F
*color11:    #FFD75F
! Blue
*color4:     #6479FF
*color12:    #6479FF
! Purple
*color5:     #AF5FFF
*color13:    #AF5FFF
! Cyan
*color6:     #53D3FB
*color14:    #53D3FB
! White
*color7:     #FFFFFF
*color15:    #FFFFFF

! URxvt
URxvt.saveLines:         100000
URxvt.scrollBar:         false
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true
URxvt.letterSpace:       -1
URxvt.font:              xft:Hack-Regular:size=9
URxvt.boldFont:          xft:Hack-Bold:size=9:weight=bold
URxvt.italicFont:        xft:Hack-RegularOblique:size=9:slant=italic
URxvt.internalBorder:    4
URxvt.perl-ext:
URxvt.perl-ext-common:

